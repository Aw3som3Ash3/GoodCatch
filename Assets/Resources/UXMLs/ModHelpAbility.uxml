            <ui:VisualElement style="flex-grow: 1; background-image: none; -unity-background-scale-mode: scale-to-fit; align-self: stretch; flex-wrap: wrap; align-items: center; justify-content: flex-end;">
                <ui:VisualElement name="Icon" style="flex-grow: 1; align-self: stretch; background-image: url(&quot;project://database/Assets/HelpMenuScreenshots/abilities2.png?fileID=2800000&amp;guid=d8cf63c80095e974fae0d3730a2cf71b&amp;type=3#abilities2&quot;); -unity-background-scale-mode: scale-to-fit; max-height: 700px; height: 700px;" />
