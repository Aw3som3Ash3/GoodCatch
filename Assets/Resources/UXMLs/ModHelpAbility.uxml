            <ui:VisualElement style="flex-grow: 1; background-image: none; -unity-background-scale-mode: scale-to-fit; align-self: stretch; flex-wrap: wrap; align-items: center; justify-content: flex-end;">
                <ui:VisualElement name="Icon" style="flex-grow: 1; align-self: stretch; background-image: url(&quot;project://database/Assets/HelpMenuScreenshots/abilities1.png?fileID=2800000&amp;guid=9193152a3574776499a7b5adfb29bbb0&amp;type=3#abilities1&quot;); -unity-background-scale-mode: scale-to-fit; max-height: 700px; height: 700px;" />
                <ui:Label tabindex="-1" text="Example of abilities and the tooltip. Treasure Trove can only be used from the middle lane and to all 3 lanes.&#10;" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Description" style="flex-wrap: wrap; -unity-text-align: upper-center;" />
            </ui:VisualElement>
        </ui:VisualElement>
