#unity-tabs-container {
    background-color: rgba(255, 255, 255, 0);
}

.unity-tab-button__content:hover {
    background-color: rgb(255, 0, 0);
}

#Tab1:hover {
    background-color: rgba(255, 0, 0, 0);
    min-width: auto;
    max-width: none;
    -unity-text-outline-color: rgb(0, 0, 0);
}

#Tab2:hover {
    background-color: rgba(255, 0, 0, 0);
}

#Tab3:hover {
    background-color: rgba(255, 0, 0, 0);
}

#Label {
    margin-left: 27px;
    font-size: 24px;
}

.unity-tab-button__content:focus {
    background-image: url("project://database/Assets/Prefabs/UI/UI-Png/Smeb.png?fileID=2800000&guid=d4b26366d79863e479e2919ca6c5ecce&type=3#Smeb");
    -unity-background-image-tint-color: rgb(255, 0, 0);
}

.unity-tab-button__top-bar {
    background-color: rgba(0, 0, 0, 0);
    width: 0;
}

.unity-tab-button--active.unity-tab-button__top-bar {
    background-color: rgba(0, 0, 0, 0);
    border-left-color: rgba(255, 95, 95, 0);
    border-right-color: rgba(255, 95, 95, 0);
    border-top-color: rgba(255, 95, 95, 0);
    border-bottom-color: rgba(255, 95, 95, 0);
}

