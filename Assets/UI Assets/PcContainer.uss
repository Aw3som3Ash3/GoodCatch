.unity-tab-button {
    color: rgb(0, 0, 0);
    margin-top: -19px;
}

.unity-tab-button__content {
    width: 126px;
    margin-right: 75px;
    min-height: 123px;
    background-color: rgba(44, 58, 111, 0.71);
    color: rgb(255, 255, 255);
    background-image: url("project://database/Assets/Prefabs/UI/UI-Png/Smeb.png?fileID=2800000&guid=d4b26366d79863e479e2919ca6c5ecce&type=3#Smeb");
}

.unity-tabbed-view__tabs-container {
    align-items: auto;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    color: rgb(255, 255, 255);
    min-height: auto;
    -unity-text-outline-color: rgba(255, 119, 119, 0);
    background-color: rgb(255, 0, 0);
}

.unity-tab-button:hover {
    color: rgb(0, 0, 0);
    margin-top: -19px;
    scale: 1.6 1.6;
    transform-origin: bottom;
    transition-duration: 0.1s;
}

.unity-tab-button:focus {
    color: rgb(0, 0, 0);
    margin-top: -19px;
    scale: 1.6 1.6;
    transform-origin: bottom;
    transition-duration: 0.1s;
}

.unity-tab-button--active {
    bottom: 0;
    transform-origin: bottom;
    scale: 1.5 1.5;
    color: rgb(255, 255, 255);
    background-color: rgba(255, 0, 0, 0);
    -unity-background-image-tint-color: rgb(255, 255, 255);
}

#unity-content-container {
    align-self: stretch;
}

.Aquarium-Box {
    flex-direction: row;
    flex-wrap: wrap;
    align-items: flex-start;
}

.Aquarium-slot {
    align-items: auto;
}

.Aquarium-slot:hover {
    scale: 1.1 1.1;
    transition-duration: 0.1s;
}

.Aquarium-slot:focus {
    scale: 1.1 1.1;
}

.Aquarium-slot__Selected {
    scale: 1.2 1.2;
}

